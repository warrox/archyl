generator client {
  provider = "prisma-client"
  output   = "./client/prisma"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://Warren:LikeSugarOnMyTongue@localhost:5432/Archyl_dbTelegram"
}


model User {
  id    Int     @id
  name  String
  tags  UserTag[]
  createdAt  DateTime  @default(now())
  updatedAt  DateTime  @updatedAt
}

model UserTag {
  id      Int    @id @default(autoincrement())
  user    User   @relation(fields: [userId], references: [id])
  userId  Int
  name    String
  content String
}
